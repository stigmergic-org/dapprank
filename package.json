{
  "name": "dapprank-workspace",
  "version": "2.3.0",
  "description": "DappRank workspace - ENS site analysis and ranking",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter frontend run start",
    "build": "pnpm --filter frontend run build",
    "analyze": "pnpm --filter dapprank run analyze",
    "scan": "pnpm --filter dapprank run scan -d ../frontend/public/dapps -c ../llm-cache"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
